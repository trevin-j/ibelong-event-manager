{
    "name": "I-Belong Event Manager",
    "description": "An event manager to help keep track of activities that will be published to I-Belong.",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}